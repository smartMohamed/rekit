{
  "name": "rekit",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@smartfrog/password": "^1.1.0",
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "build-storybook -c .storybook -o dist",
    "test": "jest",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.6",
    "@storybook/addon-knobs": "^4.0.6",
    "@storybook/addon-links": "^4.0.6",
    "@storybook/addon-options": "^4.0.8",
    "@storybook/react": "^4.0.6",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest": "^23.6.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|styl)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(spec))\\.jsx?$",
    "mapCoverage": true
  }
}